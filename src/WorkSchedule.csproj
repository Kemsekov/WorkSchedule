<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Library</OutputType>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Version>1.0.0</Version>
        <PackageId>Kemsekov.WorkSchedule</PackageId>
        <PackageVersion>$(Version)</PackageVersion>
        <Authors>Kemsekov</Authors>
        <PackageProjectUrl>https://github.com/Kemsekov/WorkSchedule</PackageProjectUrl>
    </PropertyGroup>
    <PropertyGroup Condition="Exists('$(PackageReleaseNotes)')">
        <PackageReleaseNotes>$(PackageReleaseNotes)</PackageReleaseNotes>
    </PropertyGroup>
</Project>
